<section class="section-md bg-white">
    <div class="shell shell-wide">
        <div class="range">
            <div class="cell-md-8">
                <blockquote class="quote-classic">
                    <div class="quote-classic-left">
                        <img class="quote-classic-image" src="/img/@(Model.User.Image ?? "aqua-paw-print-md.png")" alt="" width="280" height="320">
                    </div>
                    <div class="quote-classic-body">
                        <div class="quote-classic-header">
                            <cite>@Model.Content</cite>
                            <p class="caption">and Delta</p>
                        </div>
                        <div class="quote-classic-meta">
                            <ul class="post-meta">
                                <li>
                                    <span class="icon icon-xs icon-tan-hide material-icons-access_time"></span>
                                    <time datetime="2016-01-01">@Model.CreateDate.ToString("dd MMMM yyyy")</time>
                                </li>
                                <div class="likes pull-right" style="width: 133px">
                                    <div class="like grow like-reaction @(long.TryParse(ViewContext.HttpContext.User.Identity.GetUserId(), out long id) && Model.Reactions.Any(x=>x.UserId == id && x.Liked) ? "active" : "")" data-url="@Url.Action("AddReaction", "Account", new { id = Model.Id, content=nameof(Questions), like=true })">
                                        <i class="fa fa-thumbs-up fa-2x " aria-hidden="true"></i>
                                        <span class="likes-count">@Model.Reactions.Count(x => x.Liked)</span>
                                    </div>
                                    <!-- Thumbs down -->
                                    <div class="dislike grow  like-reaction @(long.TryParse(ViewContext.HttpContext.User.Identity.GetUserId(), out id) && Model.Reactions.Any(x=>x.UserId == id && !x.Liked) ? "active" : "")" data-url="@Url.Action("AddReaction", "Account", new { id = Model.Id, content=nameof(Questions), like=false })">
                                        <i class="fa fa-thumbs-down fa-2x"  aria-hidden="true"></i>
                                        <span class="dislikes-count">@Model.Reactions.Count(x => !x.Liked)</span>
                                    </div>
                                </div>
                            </ul>
                            <ul class="post-meta">
                                <li>
                                    <span class="big text-blue-marguerite">@Model.AnimalType.Description</span>
                                </li>
                                <li>
                                    <span class="big text-blue-marguerite">@Model.Category.Description</span>
                                </li>
                            </ul>

                        </div>

                    </div>

                </blockquote>
                <div class="panel panel-custom panel-corporate">
                    @if (Model.Image.Any())
                    {
                        <div class="panel-heading" id="accordion1Heading1" role="tab">
                            <div class="panel-title">
                                <a role="button" data-toggle="collapse" data-parent="#accordion1" href="https://livedemo00.template-help.com/wt_61279/host-your-own-fundraiser.html#accordion1Collapse1" aria-controls="accordion1Collapse1" aria-expanded="false" class="collapsed">
                                    This question has photos attached
                                    <div class="panel-arrow"></div>
                                </a>
                            </div>
                        </div>
                        <div class="panel-collapse collapse" id="accordion1Collapse1" role="tabpanel" aria-labelledby="accordion1Heading1" aria-expanded="false" style="height: 0px;">
                            <div class="panel-body">
                                <div class="row  spacing-10 flickr" data-flickr-tags="tm-61279" data-photo-swipe-gallery="gallery">
                                    @foreach (var image in Model.Image)
                                    {
                                        <div class="col-xs-4">
                                            <div class="flickr-item" data-type="flickr-item">
                                                <a class="thumbnail-flickr" data-image_b="href" data-photo-swipe-item="" href="~/img/@image" data-size="1023x884">
                                                    <figure><img src="~/img/@image" data-image_m="src" data-title="alt" width="165" height="165" alt="Pretty Kitty"></figure>
                                                    <div class="thumbnail-overlay"></div>
                                                </a>
                                            </div>
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    }
                </div>
                @if (answer != null)
                {
                    <div class="cell-md-8 quote-classic-body" style="margin-top: 30px">
                        <h3>Answer</h3>
                        <blockquote class="quote-primary">
                            <div class="quote-primary-body">
                                <svg class="quote-primary-mark" version="1.1" baseProfile="tiny" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="21px" height="15px" viewBox="0 0 21 15" overflow="scroll" xml:space="preserve" preserveAspectRatio="none">
                                <path d="M9.597,10.412c0,1.306-0.473,2.399-1.418,3.277c-0.944,0.876-2.06,1.316-3.349,1.316                         c-1.287,0-2.414-0.44-3.382-1.316C0.482,12.811,0,11.758,0,10.535c0-1.226,0.58-2.716,1.739-4.473L5.603,0H9.34L6.956,6.37                         C8.716,7.145,9.597,8.493,9.597,10.412z M20.987,10.412c0,1.306-0.473,2.399-1.418,3.277c-0.944,0.876-2.06,1.316-3.35,1.316                         c-1.288,0-2.415-0.44-3.381-1.316c-0.966-0.879-1.45-1.931-1.45-3.154c0-1.226,0.582-2.716,1.74-4.473L16.994,0h3.734l-2.382,6.37                         C20.106,7.145,20.987,8.493,20.987,10.412z"></path>
                        </svg>
                                <div class="quote-primary-text">
                                    <q>@answer.Content</q>
                                </div>
                            </div>
                            <div class="quote-primary-footer">
                                <img src="~/img/@(answer.User.Image ?? "aqua-paw-print-md.png")" alt="" width="80" height="80" style="margin-right: 20px">
                                <cite>@answer.User.FirstName @answer.User.LastName</cite>

                                <ul class="post-meta">
                                    <li>
                                        <span class="icon icon-xs icon-tan-hide material-icons-access_time"></span>
                                        <time datetime="2016-01-01">@answer.CreateDate.ToString("dd MMMM yyyy")</time>
                                    </li>
                                    <div class="likes pull-right" style="width: 133px">
                                        <div class="like grow like-reaction @(long.TryParse(ViewContext.HttpContext.User.Identity.GetUserId(), out id) && answer.Reactions.Any(x=>x.UserId == id && x.Liked) ? "active" : "")"  data-url="@Url.Action("AddReaction", "Account", new { id = answer.Id, content=nameof(Comments), like=true })">
                                            <i class="fa fa-thumbs-up fa-2x " aria-hidden="true"></i>
                                            <span class="likes-count">@answer.Reactions.Count(x => x.Liked)</span>
                                        </div>
                                        <!-- Thumbs down -->
                                        <div class="dislike grow like-reaction @(long.TryParse(ViewContext.HttpContext.User.Identity.GetUserId(), out id) && answer.Reactions.Any(x=>x.UserId == id && !x.Liked) ? "active" : "")"  data-url="@Url.Action("AddReaction", "Account", new { id = answer.Id, content=nameof(Comments), like=false })">
                                            <i class="fa fa-thumbs-down fa-2x " aria-hidden="true"></i>
                                            <span class="dislikes-count">@answer.Reactions.Count(x => !x.Liked)</span>
                                        </div>
                                    </div>
                                </ul>
                            </div>
                        </blockquote>

                    </div>
                }

            </div>
            <div class="col-md-4">
                <div class="cell-xs-12 height-fill bg-blue-marguerite" style="margin-top: 20px">
                    <div class="counter-box-bold">
                        <div class="counter  animated" data-from="0" data-to="2.5" data-formatter="false">ASK</div>
                        <p class="counter-box-header">a vet</p>
                        <a asp-controller="Account" asp-action="AddQuestion" class="btn btn-silver-outline btn-circle">Click here to ask a vet</a>
                    </div>
                </div>
                <form class="rd-search form-sm search-form-classic" action="search-results.html" method="GET">
                    <div class="panel-group panel-group-custom panel-group-corporate" id="accordion1" role="tablist" aria-multiselectable="true" style="margin-top: 30px">
                        <h2>Related questions</h2>

                        <div class="range range-xs-center spacing-30 text-center">
                            <div class="cell-xs-10 cell-sm-6 cell-md-12">
                                <ul class="list-md">
                                    @foreach (var relatedQuestion in relatedQuestions)
                                    {
                                        <li>
                                            <article class="post-preview">
                                                <div class="unit unit-spacing-xs unit-horizontal unit-middle">
                                                    <div class="unit-left">
                                                        <img class="post-preview-image" src="~/img/@(relatedQuestion.User.Image ?? "aqua-paw-print-md.png")" alt="" width="103" height="60">
                                                    </div>
                                                    <div class="unit-body">
                                                        <p class="post-preview-title">
                                                            <a href="@Url.Action("Question", "Forum", new {id = relatedQuestion.Id})">@(string.Join("", relatedQuestion.Content.Take(30)) + "...")</a>
                                                        </p>
                                                    </div>
                                                </div>
                                            </article>
                                        </li>
                                    }
                                </ul>
                                <a href="@Url.Action("Index", "Forum",
                                        new QuestionFilter {
                                        AnimalTypeId = new List<long?>{ Model.AnimalTypeId }.ToArray() ,
                                        CategoryId = new List<long?> { Model.CategoryId }.ToArray()})" class="btn btn-circle btn-silver-outline btn-shadow">View more</a>
                            </div>

                        </div>
                    </div>
                </form>

            </div>
        </div>
        <div class="shell shell-wide" style="margin-top: 20px">
            <hr>
        </div>
        <div class="shell shell-wide" style="margin-top: 20px">
            <div class="range spacing-55">
                <div class="cell-md-7 cell-lg-8">
                    <div class="blog-main">
                        <div class="blog-main-item content-list">
                            @if (Model.Comments.Any(x => x.Id != answer?.Id))
                            {

                                <h4>More answers</h4>
                                @foreach (var comment in Model.Comments.Where(x => x.Id != answer?.Id).OrderByDescending(x => x.Rating))
                                 {
                                     @Html.Partial("~/Views/Partials/Comment.cshtml", comment)
                                 }

                                
                            }
                        </div >
                        <div class="blog-main-item">
                            <h4>Send a Comment</h4>
                            <!-- RD Mailform-->
                            <form class="form-wide add-comment-form" data-form-output="form-output-global" data-form-type="contact" method="post" action="@Url.Action("AddComment", "Account")" novalidate="novalidate">
                                <div class="range range-sm-bottom spacing-30">
                                    <div class="cell-xs-12">
                                        <div class="form-group">
                                            <input hidden name="QuestionId" value="@Model.Id"/>
                                            <textarea class="form-control form-control-has-validation" id="comment-message" name="content" data-constraints="@@Required"></textarea><span class="form-validation"></span>
                                            <label class="form-label rd-input-label" for="comment-message">Message</label>
                                        </div>
                                    </div>
                                    <div class="cell-xs-12">
                                        <button class="btn btn-primary btn-effect-ujarak btn-block" type="submit">Submit</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                <div class="cell-md-4">
                    <a class="banner" href="#"><img src="~/img/@(new Random().Next(1, 14)).jpg" alt="" width="370" height="518"></a>
                </div>

            </div>
        </div>
    </div>

</section>
